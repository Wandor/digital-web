{
    "emulators": {
        "functions": {
            "port": 5001
        },
        "hosting": {
            "port": 5000
        },
        "pubsub": {
            "port": "firebase deploy"
        }
    },
    "hosting": {
        "public": "public",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [{
            "source": "**",
            "destination": "/index.html"
        }]
    }
}